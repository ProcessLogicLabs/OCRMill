{
  "input_folder": "input",
  "output_folder": "output",
  "database_path": "Resources/parts_database.db",
  "poll_interval": 10,
  "auto_start": false,
  "consolidate_multi_invoice": true,
  "auto_cbp_export": false,
  "check_updates_on_startup": true,
  "cbp_export": {
    "input_folder": "output/Processed",
    "output_folder": "output/CBP_Export"
  },
  "templates": {
    "mmcite_czech": {
      "enabled": true
    },
    "mmcite_brazilian": {
      "enabled": true
    }
  },
  "window": {
    "width": 1920,
    "height": 917,
    "x": 8,
    "y": 0
  },
  "parts_master_columns": {
    "part_number": true,
    "description": true,
    "hts_code": true,
    "country_origin": true,
    "mid": true,
    "client_code": false,
    "steel_pct": true,
    "aluminum_pct": true,
    "copper_pct": false,
    "wood_pct": false,
    "auto_pct": false,
    "non_steel_pct": true,
    "qty_unit": true,
    "sec301_exclusion_tariff": false,
    "fsc_certified": true,
    "fsc_certificate_code": true,
    "last_updated": true
  },
  "licensing": {
    "require_login": false,
    "allow_skip_login": true
  },
  "billing": {
    "enabled": true,
    "remote_sync_enabled": false,
    "config_repo_path": null
  },
  "auth": {
    "allowed_domains": [],
    "auto_windows_login": true
  }
}